{
    "name" : "WazzupWeb Dark",
    "version" : "0.1",
    "description" : "Set WhatsApp Web to dark mode theme.",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
          "matches": [
            "https://web.whatsapp.com/"
          ],
          "js": ["jquery-3.5.1.min.js", "popup.js"]
        } 
      ]
  }
  